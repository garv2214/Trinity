{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Backend Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/backend/server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "restart": true,
      "console": "integratedTerminal"
    },
    {
      "name": "Attach to Node Process",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}"
    }
  ]
}
